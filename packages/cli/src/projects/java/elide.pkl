amends "elide:project.pkl"

name = "elide-test-java"
description = "Example project using Elide with Java."

entrypoint {
  "src/main/java/sample/Hello.java"
}

tests {
  coverage {
    paths {
      "sample*"
    }
  }
}

sources {
  ["main"] = "src/main/java/**/*.java"
  ["test"] = "src/test/java/**/*.java"
}
