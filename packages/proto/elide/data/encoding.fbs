/*
 * Specifies data encoding definitions, enumerations, and structures.
 */
namespace elide.data;

// Data Encoding
//
// Describes supported file encodings. By default, files are encoded with `UTF-8`.
enum Encoding : int {
  // Default encoding: UTF-8.
  UTF_8 = 0,

  // Encoding: UTF-16.
  UTF_16 = 1,

  // Encoding: UTF-32.
  UTF_32 = 2,
}
